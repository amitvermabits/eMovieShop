<hibernate-mapping>
  <class name="com.calance.entity.DueDays" table="emshop.due_days_mst">
   <id name="id" column="id" >
   	<generator class="increment"/>
  </id>

 <property name="item_id">
  <column name="item_id"></column>
 </property>
 
 <property name="due_in_days">
  <column name="due_in_days"></column>
 </property>
   
       
  </class>
</hibernate-mapping>
